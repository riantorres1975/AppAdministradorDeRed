<AnchorPane id="AnchorPane" prefHeight="465.0" prefWidth="338.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Registro_Falla.RegistroController">
    <stylesheets>
        <URL value="@registro.css" />
    </stylesheets>
   <children>
      <Button fx:id="agregar" layoutX="124.0" layoutY="293.0" mnemonicParsing="false" onAction="#boton_agregar" text="Agregar " />
      <TextArea fx:id="descri" layoutX="32.0" layoutY="71.0" prefHeight="214.0" prefWidth="248.0" promptText="Descripcion" />
      <Button layoutX="84.0" layoutY="332.0" mnemonicParsing="false" onAction="#boton_historial" prefHeight="40.0" prefWidth="143.0" text="Ver Registros de Fallas" />
